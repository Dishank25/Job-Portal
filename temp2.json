        {
          "name": "Verify OTP (Mobile)",
          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "pm.test(\"Response status code is 200\", function () {\r",
                  "    pm.expect(pm.response.code).to.equal(200);\r",
                  "});\r",
                  "\r",
                  "\r",
                  "pm.test(\"Response contains required fields: message and userDetail\", function () {\r",
                  "    const responseData = pm.response.json();\r",
                  "    \r",
                  "    pm.expect(responseData).to.be.an('object');\r",
                  "    pm.expect(responseData).to.have.all.keys('message', 'userDetail');\r",
                  "});\r",
                  "\r",
                  "\r",
                  "pm.test(\"UserDetail Object Structure and Required Fields Validation\", function () {\r",
                  "    const responseData = pm.response.json();\r",
                  "    \r",
                  "    pm.expect(responseData).to.be.an('object');\r",
                  "    pm.expect(responseData.userDetail).to.be.an('object');\r",
                  "\r",
                  "    // Check for existence of properties in userDetail\r",
                  "    pm.expect(responseData.userDetail).to.have.all.keys(\r",
                  "        'id', 'user_id', 'firstName', 'lastName', 'email', \r",
                  "        'phone', 'dob', 'aadhaarNumber', 'aadhaarCardFile', \r",
                  "        'isAadhaarVerified', 'currentLocationId', 'jobLocationId', \r",
                  "        'gender', 'userType', 'salaryDetails', 'currentlyLookingFor', \r",
                  "        'workMode', 'aboutus', 'careerObjective', 'resume', \r",
                  "        'language', 'isEmailVerified', 'isPhoneVerified', \r",
                  "        'isGstVerified', 'userprofilepic', 'termsAndCondition', \r",
                  "        'createdAt', 'updatedAt', 'experiences', 'educations'\r",
                  "    );\r",
                  "\r",
                  "    // Validate types of specific fields\r",
                  "    pm.expect(responseData.userDetail.id).to.be.a('number');\r",
                  "    pm.expect(responseData.userDetail.user_id).to.be.a('number');\r",
                  "    pm.expect(responseData.userDetail.firstName).to.be.a('string');\r",
                  "    pm.expect(responseData.userDetail.lastName).to.be.a('string');\r",
                  "    pm.expect(responseData.userDetail.email).to.match(/^[^\\s@]+@[^\\s@]+\\.[^\\s@]+$/); // Email format\r",
                  "    pm.expect(responseData.userDetail.phone).to.be.a('string');\r",
                  "    pm.expect(responseData.userDetail.dob).to.be.a('string');\r",
                  "    pm.expect(responseData.userDetail.isAadhaarVerified).to.be.a('boolean');\r",
                  "    pm.expect(responseData.userDetail.currentLocationId).to.be.a('number');\r",
                  "    pm.expect(responseData.userDetail.jobLocationId).to.be.a('number');\r",
                  "    pm.expect(responseData.userDetail.isEmailVerified).to.be.a('boolean');\r",
                  "    pm.expect(responseData.userDetail.isPhoneVerified).to.be.a('boolean');\r",
                  "    pm.expect(responseData.userDetail.isGstVerified).to.be.a('boolean');\r",
                  "    pm.expect(responseData.userDetail.termsAndCondition).to.be.a('boolean');\r",
                  "    \r",
                  "    // Validate array fields\r",
                  "    pm.expect(responseData.userDetail.experiences).to.be.an('array');\r",
                  "    pm.expect(responseData.userDetail.educations).to.be.an('array');\r",
                  "    pm.expect(responseData.skills).to.be.an('array');\r",
                  "});\r",
                  "\r",
                  "\r",
                  "pm.test(\"Response time is less than 200ms\", function () {\r",
                  "    pm.expect(pm.response.responseTime).to.be.below(200);\r",
                  "});\r",
                  "\r",
                  "\r",
                  "pm.test(\"Experiences and Educations are valid arrays\", function () {\r",
                  "    const responseData = pm.response.json();\r",
                  "    \r",
                  "    // Check that experiences is an array and validate its contents\r",
                  "    pm.expect(responseData.userDetail.experiences).to.be.an('array');\r",
                  "    responseData.userDetail.experiences.forEach(experience => {\r",
                  "        pm.expect(experience).to.be.an('object').that.is.empty; // Assuming valid objects are empty for this test\r",
                  "    });\r",
                  "\r",
                  "    // Check that educations is an array and validate its contents\r",
                  "    pm.expect(responseData.userDetail.educations).to.be.an('array');\r",
                  "    responseData.userDetail.educations.forEach(education => {\r",
                  "        pm.expect(education).to.be.an('object').that.is.empty; // Assuming valid objects are empty for this test\r",
                  "    });\r",
                  "});\r",
                  ""
                ],
